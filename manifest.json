{
	"manifest_version": 2,
	"name": "People First Language",
	"version": "3",
	"description": "Put People First.",
	"icons": { 
		"16": "images/pflogo16.png",
		"48": "images/pflogo48.png",
		"128": "images/pflogo128.png" 
		},
	"content_scripts":
	[
			{
					"matches": ["*://*/*"],
					"js": ["myscript.js"],
					"run_at": "document_end"
			}
	]
}